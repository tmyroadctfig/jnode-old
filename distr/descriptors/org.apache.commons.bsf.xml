<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.apache.commons.bsf"
       name="commons-bsf"
       version="2.4"
       license-name="apache2.0"
	   provider-name="apache commons"
	   provider-url="http://commons.apache.org/commons-bsf/">

  <runtime>
    <library name="bsf.jar">
      <export name="*"/>
      <exclude name="org.apache.bsf.engines.jacl.*"/>
      <exclude name="org.apache.bsf.engines.jython.*"/>
      <exclude name="org.apache.bsf.engines.netrexx.*"/>
      <exclude name="org.apache.bsf.engines.xslt.*"/>
    </library>
  </runtime>
</plugin>
